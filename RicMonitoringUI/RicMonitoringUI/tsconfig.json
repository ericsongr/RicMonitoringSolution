        "outDir": "./dist/out-tsc",
        "sourceMap": true,
        "declaration": false,
        "moduleResolution": "node",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "module": "esnext",
        "paths": {
            "@fuse": [
                "@fuse/"
            ],
            "@auth": [
